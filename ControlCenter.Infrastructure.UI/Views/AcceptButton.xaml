<UserControl x:Class="ControlCenter.Infrastructure.UI.Views.AcceptButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ControlCenter.Infrastructure.UI.Views"
             mc:Ignorable="d" 
             d:DesignHeight="17" d:DesignWidth="17">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ImageDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Button Width="17" Height="17">
        <Image Source="{StaticResource Accept16}"/>
    </Button>
</UserControl>
